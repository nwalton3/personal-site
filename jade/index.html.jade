extends _layout

block title
	title Nate Walton

block content
	- var extension = '';
	//- var extension = '.html';

	.feature
		for quote in quotes
			.quote!= quote.text
			.byline!= quote.byline
	.content
		for block in contents
			.wrapper
				a.heading(href=block.url + extension)!= block.heading
				for image in block.images
					a.preview.toLoad(href=block.url + extension + image.hash)
						span.projectDesc
							span.projectTitle #{image.title}
						img.lazy.front(data-src=image.url, alt='')
